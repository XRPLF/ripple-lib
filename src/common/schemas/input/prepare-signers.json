{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "prepareSignersParameters",
  "type": "object",
  "properties": {
    "address": {
      "$ref": "address",
      "description": "The address of the account that is creating the transaction."
    },
    "signers": {
      "properties": {
        "quorum": {"$ref": "uint32"},
        "entries": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "properties": {
              "address": {"$ref": "address"},
              "weight": {
                "type": "integer",
                "minimum": 1,
                "maximum": 65535
              }
            },
            "additionalProperties": false
          }
        }
      },
      "additionalProperties": false
    },
    "instructions": {"$ref": "instructions"}
  },
  "additionalProperties": false,
  "required": ["address", "signers"]
}
